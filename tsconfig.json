{
  "extends": "./nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/ui/components/*": [
        "lib/components/*"
      ],
      "@/ui/layouts/*": [
        "lib/ui/layouts/*"
      ],
    },
    "strictNullChecks": true,
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}